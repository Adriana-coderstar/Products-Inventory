generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id          Int       @id @default(autoincrement())
  name        String    @db.VarChar(255)
  quantity    Int
  id_provider Int
  category    String    @db.VarChar(255)
  provider    Providers @relation(fields: [id_provider], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "products_FK")

  @@index([id_provider], map: "products_FK")
}

model Providers {
  id       Int        @id @default(autoincrement())
  company  String     @db.VarChar(255)
  products Products[]
}
