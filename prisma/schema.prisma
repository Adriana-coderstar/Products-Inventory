generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model products {
  id           Int      @id @default(autoincrement())
  name_product String   @db.VarChar(255)
  amount       Int
  id_provider  Int
  category     String   @db.VarChar(255)
  provider     provider @relation(fields: [id_provider], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "products_FK")

  @@index([id_provider], map: "products_FK")
}

model provider {
  id       Int        @id @default(autoincrement())
  company  String     @db.VarChar(255)
  products products[]
}
